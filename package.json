{
  "name": "react-engine",
  "version": "1.7.0",
  "description": "a composite render engine for express apps to render both plain react views and react-router views",
  "main": "index.js",
  "scripts": {
    "test": "npm run jscs && npm run lint && npm run tape",
    "jscs": "jscs --verbose .",
    "lint": "jshint --verbose --show-non-errors --config=.jshintrc **/*.js",
    "tape": "tape test/*.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paypal/react-engine.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "react": "~0",
    "react-router": "^1.0.0-beta2",
    "express": "^4.12"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "glob": "^5.0.3",
    "parent-require": "^1.0.0",
    "lodash-node": "^3.3.1"
  },
  "devDependencies": {
    "cheerio": "^0.19.0",
    "esprima-fb": "^14001.1.0-dev-harmony-fb",
    "faucet": "0.0.1",
    "jscs": "^1.11.3",
    "jsdom": "3.0.0",
    "jshint": "^2.6.3",
    "rewire": "^2.3.1",
    "sinon": "^1.14.1",
    "tape": "^3.5.0",
    "react": "~0",
    "react-router": "^1.0.0-beta2",
    "express": "^4.12"
  },
  "keywords": [
